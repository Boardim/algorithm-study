#include <iostream>
using namespace std;
void bubbleSort(int arr[], int n) {
	int temp;
	for (int i = 0; i < n - 1; i++) {
		for (int k = 0; k < n - 1 - i; k++) {
			if (arr[k] > arr[k + 1]) {
				temp = arr[k];
				arr[k] = arr[k + 1];
				arr[k + 1] = temp;
			}
			/* 버블 정렬 과정 출력
			for (int j = 0; j < n; j++) {
				cout << arr[j] << " ";
			}
			cout << endl << endl;  
			*/ 
		}
	}
}

int main() {
	int arr[5] = {5,3,2,4,7};
	int n{ 5 };
	bubbleSort(arr, n);
	cout << "bubbleSort result is: ";
	for (int i = 0; i < n; i++) {
		cout << arr[i] << " ";
	}
}
