#오름차순 정렬
#include <iostream>
using namespace std;
void insertionSort(int arr[], int n) {
	int temp;
	for (int i = 1; i < n; i++) {
		for (int k = i; k > 0; k--) {
			if (arr[k] < arr[k-1]) {
				temp = arr[k-1];
				arr[k-1] = arr[k];
				arr[k] = temp;
				/*
				for (int i = 0; i < 5; i++) {
					cout << arr[i] << " ";
				}
				cout << endl;
				과정 출력 */
			}
		}
	}
}

int main() {
	int arr[5] = { 4,7,2,5,1 };
	insertionSort(arr, 5);
	for (int i = 0; i < 5; i++) {
		cout << arr[i] << " ";
	}
}
